{
  "name": "webbapp",
  "version": "1.0.0",
  "scripts": {
    "build:sass": "sass wwwroot/scss/main.scss:wwwroot/css/site.css --style=compressed --no-source-map",
    "watch:sass": "sass --watch wwwroot/scss/main.scss:wwwroot/css/site.css --no-source-map",
    "start": "run-p watch:sass"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "sass": "^1.58.3"
  }
}