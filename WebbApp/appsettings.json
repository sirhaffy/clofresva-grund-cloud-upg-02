{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MongoDb": {
    "ConnectionString": "mongodb://{connectionstring}",
    "DatabaseName": "{database}",
    "SubscribersCollectionName": "{collection}"
  },
  "AzureBlob": {
    "ContainerUrl": "https://{accountname}.blob.core.windows.net/{container}"
  }
}
