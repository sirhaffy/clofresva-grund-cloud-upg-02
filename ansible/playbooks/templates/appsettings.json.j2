{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Storage": {
    "AccountName": "{{ storage_account | default('') }}",
    "BlobEndpoint": "{{ blob_endpoint | default('') }}"
  },
  "MongoDB": {
    "ConnectionString": "{{ mongodb_connection_string | default('') }}"
  },
  "AppSettings": {
    "Environment": "{{ app_environment | default('Production') }}"
  }
}